{
    "mdl": "Smart Plug+",
    "mdlpn": "MIHO005",
    "parameters": [
       {
          "id": "switch",
          "component": "switch",
          "stat_t": "~@/state",
          "cmd_t": "~@/command",
          "main": true,
          "config": {
             "dev_cla": "outlet",
             "icon": "mdi:power-socket-uk",
             "opt": false
          }
       },
       {
          "id": "REAL_POWER",
          "component": "sensor",
          "stat_t": "~@/state",
          "config": {
             "dev_cla": "power",
             "unit_of_meas": "W"
          }
       },
       {
          "id": "REACTIVE_POWER",
          "component": "sensor",
          "stat_t": "~@/state",
          "config": {
             "dev_cla": "reactive_power",
             "unit_of_meas": "var"
          }
       },
       {
          "id": "FREQUENCY",
          "component": "sensor",
          "stat_t": "~@/state",
          "config": {
             "val_tpl": "{{ value|round(2) }}",
             "dev_cla": "frequency",
             "unit_of_meas": "Hz",
             "en": "false"
          }
       },
       {
          "id": "VOLTAGE",
          "component": "sensor",
          "stat_t": "~@/state",
          "config": {
             "dev_cla": "voltage",
             "unit_of_meas": "V",
             "en": "false"
          }
       }
    ]
 }